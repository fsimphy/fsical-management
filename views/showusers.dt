extends layout.dt
block content
	h1 Users
	table
		tr
			th id
			th username
			th privilege
		- foreach (user; users)
			tr
				td #{user.id}
				td #{user.username}
				td #{user.privilege}
				td
					form(action="/removeuser", method="post")
						input#id(value="#{user.id}", name="id", type="hidden")
						input#submitButton(type="submit", value="Entfernen")
	- if (_error)
		p.error= _error
